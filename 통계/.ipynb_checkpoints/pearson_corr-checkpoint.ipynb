   "source": [
    "def pearson_cor(x,y):\n",
    "    x_bar = np.mean(x)\n",
    "    y_bar = np.mean(y)\n",
    "    \n",
    "    Cov = np.sum([(x-x_bar)*(y-y_bar) for x,y in zip(x,y)]) / (len(x)-1)\n",
    "    print(Cov)"
   ]
  }
